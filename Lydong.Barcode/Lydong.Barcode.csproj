<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Authors>lyd</Authors>
    <Description>生成，解析，一维码，二维码。</Description>
    <PackageProjectUrl>https://github.com/Lyd889911/Lydong.Barcode</PackageProjectUrl>
    <PackageIcon>ico.png</PackageIcon>
    <RepositoryUrl>https://github.com/Lyd889911/Lydong.Barcode</RepositoryUrl>
    <PackageTags>Barcode;QRCode</PackageTags>
    <Title>Lydong.Barcode</Title>
    <Version>1.0.2</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="ZXing.Net.Bindings.SkiaSharp" Version="0.16.14" />
  </ItemGroup>

  <ItemGroup>
    <None Update="ico.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
